package com.lx.entity.home;

public class Distance {
	private int id;
	private String startAddress;//开始地址
	private String endAddress;//结束地址
	private Integer distance;//里程数
	private int addKm;
	private int addMuch;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getStartAddress() {
		return startAddress;
	}
	public void setStartAddress(String startAddress) {
		this.startAddress = startAddress;
	}
	public String getEndAddress() {
		return endAddress;
	}
	public void setEndAddress(String endAddress) {
		this.endAddress = endAddress;
	}
	public Integer getDistance() {
		return distance;
	}
	public void setDistance(Integer distance) {
		this.distance = distance;
	}
	public int getAddKm() {
		return addKm;
	}
	public void setAddKm(int addKm) {
		this.addKm = addKm;
	}
	public int getAddMuch() {
		return addMuch;
	}
	public void setAddMuch(int addMuch) {
		this.addMuch = addMuch;
	}
	
	
	
	

}
